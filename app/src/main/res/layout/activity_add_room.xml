        app:layout_constraintBottom_toBottomOf="@+id/bc1"
        app:layout_constraintEnd_toEndOf="@+id/bc1"
        app:layout_constraintStart_toStartOf="@+id/bc1"
        app:layout_constraintTop_toTopOf="@+id/bc1"
        app:srcCompat="@drawable/kitchen" />

    <ImageView
        android:id="@+id/imageBathroom"
        android:layout_width="56dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/bc3"
        app:layout_constraintEnd_toEndOf="@+id/bc3"
        app:layout_constraintStart_toStartOf="@+id/bc3"
        app:layout_constraintTop_toTopOf="@+id/bc3"
        app:srcCompat="@drawable/bathroom" />

    <ImageView
        android:id="@+id/imageOffice"
        android:layout_width="56dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/bc4"
        app:layout_constraintEnd_toEndOf="@+id/bc4"
        app:layout_constraintStart_toStartOf="@+id/bc4"
        app:layout_constraintTop_toTopOf="@+id/bc4"
        app:srcCompat="@drawable/office" />

    <ImageView
        android:id="@+id/imageTv"
        android:layout_width="56dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/bc5"
